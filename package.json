{
  "name": "luban-ts-service",
  "version": "0.0.6-0",
  "description": "generate typescript interface and service function/class on yapi doc",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "bin": {
    "lts": "bin/index.js"
  },
  "scripts": {
    "eslint": "eslint src/**/*.ts",
    "compile": "tsc --noEmit --diagnostics",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "build": "rimraf lib && cross-env NODE_ENV=production tsc --pretty",
    "watch": "npm run build -- --watch --diagnostics",
    "gen": "node bin/index.js gen",
    "release:beta": "togo-np prerelease --tag beta --run-scripts 'eslint build' --allow-any-branch",
    "release": "togo-np patch --tag latest --run-scripts 'eslint build' --branch main"
  },
  "dependencies": {
    "@luban-cli/cli-shared-utils": "^1.3.3",
    "chalk": "^4.1.0",
    "commander": "^6.0.0",
    "dedent": "^0.7.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "json-schema": "^0.2.5",
    "json-schema-generator": "^2.0.6",
    "json-schema-to-typescript": "^9.1.1",
    "json5": "^2.1.3",
    "mockjs": "^1.1.0",
    "node-fetch": "^2.6.0",
    "qs": "^6.9.4",
    "semver": "^7.3.2",
    "vtils": "^3.32.0",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^9.0.1",
    "@types/hapi__joi": "^16.0.9",
    "@types/inquirer": "^7.3.0",
    "@types/mockjs": "^1.0.2",
    "@types/node-fetch": "^2.5.7",
    "@types/prettier": "^2.0.2",
    "@types/qs": "^6.9.4",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "commitizen": "^4.1.2",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "rimraf": "^3.0.2",
    "togo-np": "0.0.12",
    "typescript": "^3.8.3"
  },
  "optionalDependencies": {
    "axios": "0.19.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeapFE/luban-ts-service.git"
  },
  "keywords": [
    "typescript",
    "jsonschema"
  ],
  "author": "front-end-captain",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeapFE/luban-ts-service/issues"
  },
  "homepage": "https://github.com/LeapFE/luban-ts-service#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run eslint",
      "npm run format"
    ]
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
